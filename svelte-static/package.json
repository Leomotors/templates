{
  "name": "svelte-static-template",
  "description": "Template for making Static SveltKit Website, to be deployed at GitHub Pages",
  "version": "1.0.0",
  "type": "module",
  "author": "{username}",
  "repository": "https://github.com/{username}/{repo}",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build && node postbuild.js",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" && prettier --check \"{src,tests}/**/*.{ts,svelte,html}\" && eslint src && eslint *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" && prettier --write \"{src,tests}/**/*.{ts,svelte,html}\""
  },
  "devDependencies": {
    "@playwright/test": "^1.22.1",
    "@sveltejs/adapter-static": "^1.0.0-next.31",
    "@sveltejs/kit": "1.0.0-next.335",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "globby": "^13.1.1",
    "postcss": "^8.4.14",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}