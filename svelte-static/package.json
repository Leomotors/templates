{
  "name": "svelte-static-template",
  "description": "Template for making Static SveltKit Website, to be deployed at GitHub Pages",
  "version": "1.0.0",
  "type": "module",
  "author": "{username}",
  "repository": "https://github.com/{username}/{repo}",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build && node postbuild.js",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "rimraf tests-out/* && tsc -p tsconfig.test.json && playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" && prettier --check \"{src,tests}/**/*.{ts,svelte,html}\" && eslint src && eslint *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" && prettier --write \"{src,tests}/**/*.{ts,svelte,html}\""
  },
  "devDependencies": {
    "@playwright/test": "^1.20.1",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "1.0.0-next.303",
    "@types/tailwindcss": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "globby": "^13.1.1",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.6.0",
    "prettier-plugin-svelte": "^2.6.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.12",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  }
}